{
  "name": "tourze/symfony-cache-hotkey-bundle",
  "description": "自动扩展Cache服务",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-mbstring": "*",
    "psr/cache": "^2.0 || ^3.0",
    "psr/log": "^3|^2|^1",
    "symfony/cache": "^6.4",
    "symfony/cache-contracts": "^3",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/service-contracts": "^3.5",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/backtrace-helper": "~0.0.5"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Symfony\\CacheHotKey\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\Symfony\\CacheHotKey\\Tests\\": "tests"
    }
  }
}