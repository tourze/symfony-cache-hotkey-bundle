{
  "name": "tourze/symfony-cache-hotkey-bundle",
  "description": "自动扩展Cache服务",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "psr/log": "^3|^2|^1",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "symfony/cache": "^6.4",
    "symfony/framework-bundle": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Symfony\\CacheHotKey\\": "src"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}